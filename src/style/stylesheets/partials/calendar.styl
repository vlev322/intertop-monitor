.calendar-header
	.month
		font-size 30px
		color #403C3C
		margin 100px
	.headerWeek
		margin 30px
		display flex
		align-items center
		justify-content space-between
		border-bottom 1px solid #DDDDDD
		padding 3px 0
		li
			width 150px
			align-items: center
			font-family: "ubuntu-regular"
			font-size: 18px
.gridWeek
	margin 30px
	display flex
	align-items center
	justify-content space-between
	border-bottom 1px solid #DDDDDD
	padding 3px 0
	div
		width 150px
		height 150px
		align-items: center
		font-family: "ubuntu-regular"
		font-size: 18px
		border-bottom: 1px solid #DDDDDD
		border-right: 1px solid #DDDDDD


